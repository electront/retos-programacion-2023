#include <stdio.h>
#include <stdlib.h>

int main()
{
for(int i=1;i<=100;i++){
    if(i%3==0){
        if(i%5==0){
            printf("fizzbuzz\n");
            }
        printf("fizz\n");
        }
    else if(i%5==0){
        if(i%3==0){
            printf("fizzbuzz\n");
            }
        printf("buzz\n");
        }
    else
        printf("%i\n",i);
    }

return 0;
}
